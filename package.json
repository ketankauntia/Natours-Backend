{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning nodejs, Express and MongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Ketan Kauntia",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^4.7.0",
    "pug": "^3.0.2",
    "sharp": "^0.29.3",
    "slugify": "^1.5.3",
    "stripe": "^7.0.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
